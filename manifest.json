{
    "name": "Old Sweepy",
    "description": "Minesweeper AI",
    "version": "1.3.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://*.minesweeperonline.com/*",
                "*://*.minesweeper.online/game/*",
                "*://*.cardgames.io/minesweeper/*",
                "*://*.minesweeper.one/*"
            ],
            "js": ["ms.js"]
        }
    ],
    "action": {
        "default_title": "Old Sweepy",
        "default_popup": "popup.html",
        "default_icon": "Sweepy.png"
    },
    "permissions": ["storage"]
}
